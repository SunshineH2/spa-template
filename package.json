{
  "name": "spa-template",
  "version": "1.0.0",
  "description": "杭州雷数前端中后台管理系统模板",
  "license": "MIT",
  "author": "陈杰 <chenjie@thundersdata.com>",
  "homepage": "https://github.com/thundersdata-frontend/spa-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundersdata-frontend/spa-template.git"
  },
  "bugs": {
    "url": "https://github.com/thundersdata-frontend/spa-template/issues"
  },
  "scripts": {
    "nc-u": "npx npm-check --registry=http://npm.taobao.org/ -u",
    "commit": "git-cz",
    "start": "cross-env UMI_UI=1 umi dev",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env UMI_ENV=prod MOCK=none umi build",
    "test": "jest --passWithNoTests",
    "tsc": "tsc -p ./tsconfig.json",
    "eslint:fix": "eslint --fix --ext .ts,.tsx src/",
    "lint:fix": "prettier --check src/**/*.tsx --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "changelog": {
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run eslint:fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.10.9",
    "@ant-design/pro-table": "^2.25.1",
    "ahooks": "^2.9.4",
    "antd": "^4.10.2",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.10.3",
    "immer": "^8.0.0",
    "lodash-es": "^4.17.20",
    "lscache": "^1.3.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "umi": "^3.3.4",
    "use-immer": "^0.4.2",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/classnames": "^2.2.11",
    "@types/lodash-es": "^4.17.4",
    "@types/lscache": "^1.3.0",
    "@types/react": "16.9.43",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@umijs/fabric": "^2.4.11",
    "@umijs/plugin-esbuild": "^1.1.0",
    "@umijs/preset-react": "^1.7.9",
    "@umijs/preset-ui": "^2.2.9",
    "antd-dayjs-webpack-plugin": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.1",
    "conventional-changelog-cli": "^2.1.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "pont-engine": "^1.0.13",
    "standard-version": "^9.1.0",
    "stylelint": "^13.8.0",
    "typescript": "^4.1.3",
    "umi-plugin-page-creator": "^1.6.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
